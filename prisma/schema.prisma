generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "sqlserver"
  url      = env("DATABASE_URL")
}

model Task {
  Id          Int @id @default(autoincrement())
  TaskDescription        String @db.VarChar(255)
  CreatedAt   DateTime @default(now())
}